{
  "name": "poc-buefy",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "test:unit": "vue-cli-service test:unit --verbose --coverage=true",
    "lint": "vue-cli-service lint",
    "db": "cd server && yarn install && json-server --watch index.js",
    "start": "cd server && yarn install && json-server --watch index.js && cd .. & vue-cli-service serve",
    "docs:dev": "vuepress dev docs",
    "docs:build": "vuepress build docs"
  },
  "dependencies": {
    "axios": "^0.19.2",
    "buefy": "^0.8.13",
    "core-js": "^3.6.4",
    "leaflet": "^1.6.0",
    "vue": "^2.6.11",
    "vue-axios": "^2.1.5",
    "vue-router": "^3.1.5",
    "vue2-editor": "^2.10.2",
    "vue2-leaflet": "^2.5.2",
    "vuex": "^3.1.2",
    "vuex-persist": "^2.2.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.2.0",
    "@vue/cli-plugin-eslint": "~4.3.0",
    "@vue/cli-plugin-router": "~4.2.0",
    "@vue/cli-plugin-unit-jest": "~4.2.0",
    "@vue/cli-plugin-vuex": "~4.2.0",
    "@vue/cli-service": "~4.2.0",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/test-utils": "1.0.0-beta.31",
    "@vuepress/plugin-back-to-top": "^1.5.0",
    "@vuepress/plugin-medium-zoom": "^1.5.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^6.7.2",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-vue": "^6.2.2",
    "node-sass": "^4.9.0",
    "prettier": "^1.19.1",
    "sass-loader": "^7.0.1",
    "vue-cli-plugin-buefy": "~0.3.7",
    "vue-template-compiler": "^2.6.11",
    "vuepress": "^1.5.0",
    "vuepress-plugin-element-tabs": "^0.2.8",
    "vuepress-plugin-mermaidjs": "^1.5.1"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/strongly-recommended",
      "eslint:recommended",
      "@vue/prettier"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "overrides": [
      {
        "files": [
          "**/__tests__/*.{j,t}s?(x)",
          "**/tests/unit/**/*.spec.{j,t}s?(x)"
        ],
        "env": {
          "jest": true
        }
      }
    ],
    "rules": {
      "vue/require-default-prop": "off"
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "jest": {
    "preset": "@vue/cli-plugin-unit-jest"
  }
}
